#!/bin/sh

DOWNLOAD=$(curl https://plex.tv/api/downloads/1.json | perl -ne'/"build":"linux-ubuntu-x86_64","distro":"ubuntu","url":"(.*?)"/ && print($1) && exit')
wget $DOWNLOAD
FILE=$(echo $DOWNLOAD | perl -ne'/.*\/(.*)/ && print($1) && exit')
dpkg -i $FILE
rm $FILE

#!/bin/sh

CRASHPLAN=$(curl https://www.crashplan.com/shared/js/cp.download.js | perl -ne'/cp.download.CPPRO_CLIENT_VERSION = '\''(.*?)'\'';/ && print("https://download.code42.com/installs/linux/install/CrashPlan/CrashPlan_$1_Linux.tgz") && exit')
wget $CRASHPLAN
FILE=$(echo $CRASHPLAN | perl -ne'/.*\/(.*)/ && print($1) && exit')
tar xvfs $FILE
cd crashplan-install
chmod +x install.sh
./install.sh
cd ..
rm $FILE
rm -rf crashplan-install
cat /var/lib/crashplan/.ui_info

ssh -L 4200:localhost:4243 root@144.76.237.119
